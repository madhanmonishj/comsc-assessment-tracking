<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.assessment.comsc.mapper.UserInfoMapper">


    <select id="queryByEmail" resultType="com.assessment.comsc.entity.UserInfo">
        SELECT *
        FROM user_info
        WHERE mail = #{mail}
    </select>
    

</mapper>
